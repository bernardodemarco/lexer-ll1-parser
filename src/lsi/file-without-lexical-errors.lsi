def fibonacci(int position) {
    if (position = 0) {
        return 0;
    }

    if (position = 1) {
        return 1;
    }

    return fibonacci(position - 1) + fibonacci(position - 2);
}

def main(int argc) {
    int position, result;

    position = 5;
    result = fibonacci(position);

    print result;
}